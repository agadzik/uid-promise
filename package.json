{
  "name": "uid-promise",
  "version": "1.1.1",
  "license": "MIT",
  "description": "generates a cryptographically strong random uid",
  "repository": "vercel/uid-promise",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc --module esnext --outDir ./lib/esm",
    "test": "yarn test:cjs && yarn test:esm",
    "test:cjs": "yarn build:cjs && ava",
    "test:esm": "yarn build:esm && ava --node-arguments=\"--experimental-global-webcrypto\""
  },
  "devDependencies": {
    "@types/node": "16.6.1",
    "ava": "5.2.0",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
